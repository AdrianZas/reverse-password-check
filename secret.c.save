#include <stdio.h>
#include <string.h>

int main()
{
	char input[20];
	printf("Enter password:");
	scanf("%s", input);

	for(int i = 0; i < strlen(input); i++) {
		input[i] ^= 0x13;
	}

	if (strcmp(input, "\x50\x67\x2a\x49\x55") == 0) {
		printf("Access granted");
	} else {
		printf("Access denied");
	}
}
